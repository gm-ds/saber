                <div class="compute-header">
                    <h3>Compute ID: {{ compute_id }}<a name="{{ compute_id }}-{{ server_name }}"></a></h3>
                    <div>
                        <span class="badge">Successful Jobs: {{ compute_data.SUCCESSFUL_JOBS|length }}</span>
                        <span class="badge">Failed Jobs: {{ compute_data.FAILED_JOBS|length }}</span>
                        <span class="badge">Running Jobs: {{ compute_data.RUNNING_JOBS|length }}</span>
                        <span class="badge">New Jobs: {{ compute_data.NEW_JOBS|length }}</span>
                        <span class="badge">Queued Jobs: {{ compute_data.QUEUED_JOBS|length }}</span>
                        <span class="badge">Waiting Jobs: {{ compute_data.WAITING_JOBS|length }}</span>
                    </div>
                </div>
                
                <div class="tabs" id="tabs-{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}">
                    <div class="tab active" onclick="switchTab('{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-successful')">
                        Successful Jobs 
                        <span class="count-badge successful-badge">{{ compute_data.SUCCESSFUL_JOBS|length }}</span>
                    </div>
                    <div class="tab" onclick="switchTab('{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-failed')">
                        Failed Jobs
                        <span class="count-badge failed-badge">{{ compute_data.FAILED_JOBS|length }}</span>
                    </div>
                    <div class="tab" onclick="switchTab('{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-running')">
                        Running Jobs
                        <span class="count-badge running-badge">{{ compute_data.RUNNING_JOBS|length }}</span>
                    </div>
                    <div class="tab" onclick="switchTab('{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-new')">
                        New Jobs
                        <span class="count-badge new-badge">{{ compute_data.NEW_JOBS|length }}</span>
                    </div>
                    <div class="tab" onclick="switchTab('{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-queued')">
                        Queued Jobs
                        <span class="count-badge queued-badge">{{ compute_data.QUEUED_JOBS|length }}</span>
                    </div>
                    <div class="tab" onclick="switchTab('{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-waiting')">
                        Waiting Jobs
                        <span class="count-badge waiting-badge">{{ compute_data.WAITING_JOBS|length }}</span>
                    </div>
                </div>
                
                <!-- Successful Jobs -->
                <div id="{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-successful" class="tab-content active">
                    <h4>Successful Jobs</h4>
                    {% if compute_data.SUCCESSFUL_JOBS|length == 0 %}
                        <p>No successful jobs found.</p>
                    {% else %}
                        {% for job_id, job_data in compute_data.SUCCESSFUL_JOBS.items() %}
                            <div class="job-card">
                                <div class="job-header">
                                    <h3>Job ID: {{ job_id }} <span class="job-status-ok">(Status: {{ job_data.INFO.state }})</span></h3>
                                    <p>Tool: {{ job_data.INFO.tool_id }}</p>
                                    <div>
                                        <span class="badge">Galaxy v{{ job_data.INFO.galaxy_version }}</span>
                                        <span class="badge">Created: {{ job_data.INFO.create_time|replace('T', ' ') }}</span>
                                        <span class="badge">Updated: {{ job_data.INFO.update_time|replace('T', ' ') }}</span>
                                    </div>
                                </div>
                                <div class="job-body">
                                    <div class="info-section">
                                        <h4>Job Information</h4>
                                        <table>
                                            <tr>
                                                <th>History ID</th>
                                                <td>{{ job_data.INFO.history_id }}</td>
                                            </tr>
                                            <tr>
                                                <th>Exit Code</th>
                                                <td>{{ job_data.INFO.exit_code }}</td>
                                            </tr>
                                            <tr>
                                                <th>Command Line</th>
                                                <td>
                                                    <div class="command-line">{{ job_data.INFO.command_line }}</div>
                                                </td>
                                            </tr>
                                        </table>
                                    </div>
                                    
                                    <div class="metrics-section">
                                        <h4>Job Metrics</h4>
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>Metric</th>
                                                    <th>Value</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for metric in job_data.METRICS %}
                                                <tr class="metric-row">
                                                    <td>{{ metric.title }}</td>
                                                    <td>{{ metric.value }}</td>
                                                </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </div>
                                    
                                    <!-- Input/Output Section -->
                                    <div class="io-section">
                                        <h4>Inputs & Outputs</h4>
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>Type</th>
                                                    <th>Name</th>
                                                    <th>ID</th>
                                                    <th>UUID</th>
