                                            </tr>
                                        </table>
                                    </div>
                                    
                                    <div class="metrics-section">
                                        <h4>Job Metrics</h4>
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>Metric</th>
                                                    <th>Value</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for metric in job_data.METRICS %}
                                                <tr class="metric-row">
                                                    <td>{{ metric.title }}</td>
                                                    <td>{{ metric.value }}</td>
                                                </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </div>
                                    
                                    <!-- Input/Output Section -->
                                    <div class="io-section">
                                        <h4>Inputs & Outputs</h4>
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>Type</th>
                                                    <th>Name</th>
                                                    <th>ID</th>
                                                    <th>UUID</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for input_name, input_data in job_data.INFO.inputs.items() %}
                                                <tr>
                                                    <td>Input</td>
                                                    <td>{{ input_name }}</td>
                                                    <td>{{ input_data.id }}</td>
                                                    <td>{{ input_data.uuid }}</td>
                                                </tr>
                                                {% endfor %}
                                                
                                                {% for output_name, output_data in job_data.INFO.outputs.items() %}
                                                <tr>
                                                    <td>Output</td>
                                                    <td>{{ output_name }}</td>
                                                    <td>{{ output_data.id }}</td>
                                                    <td>{{ output_data.uuid }}</td>
                                                </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        {% endfor %}
                    {% endif %}
                </div>

                <!-- Waiting Jobs -->
                <div id="{{ server_name|replace('.', '-') }}-{{ compute_id|replace('.', '-') }}-waiting" class="tab-content">
                    <h4>Waiting Jobs</h4>
                    {% if compute_data.WAITING_JOBS|length == 0 %}
                        <p>No waiting jobs found.</p>
                    {% else %}
                        {% for job_id, job_data in compute_data.WAITING_JOBS.items() %}
                            <div class="job-card">
                                <div class="job-header">
                                    <h3>Job ID: {{ job_id }} <span class="job-status-waiting">(Status: {{ job_data.INFO.state }})</span></h3>
                                    <p>Tool: {{ job_data.INFO.tool_id }}</p>
                                    <div>
                                        <span class="badge">Galaxy v{{ job_data.INFO.galaxy_version }}</span>
                                        <span class="badge">Created: {{ job_data.INFO.create_time|replace('T', ' ') }}</span>
                                        <span class="badge">Updated: {{ job_data.INFO.update_time|replace('T', ' ') }}</span>
                                    </div>
                                </div>
                                <div class="job-body">
                                    <div class="info-section">
                                        <h4>Job Information</h4>
                                        <table>
                                            <tr>
                                                <th>History ID</th>
                                                <td>{{ job_data.INFO.history_id }}</td>
                                            </tr>
                                            <tr>
                                                <th>Exit Code</th>
                                                <td>{{ job_data.INFO.exit_code }}</td>
                                            </tr>
                                            <tr>
                                                <th>Command Line</th>
                                                <td>
                                                    <div class="command-line">{{ job_data.INFO.command_line }}</div>
                                                </td>
                                            </tr>
                                        </table>
                                    </div>
                                    
                                    <div class="metrics-section">
                                        <h4>Job Metrics</h4>
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>Metric</th>
                                                    <th>Value</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for metric in job_data.METRICS %}
                                                <tr class="metric-row">
                                                    <td>{{ metric.title }}</td>
                                                    <td>{{ metric.value }}</td>
                                                </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </div>
                                    
                                    <!-- Input/Output Section -->
                                    <div class="io-section">
                                        <h4>Inputs & Outputs</h4>
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>Type</th>
                                                    <th>Name</th>
                                                    <th>ID</th>
                                                    <th>UUID</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for input_name, input_data in job_data.INFO.inputs.items() %}
                                                <tr>
                                                    <td>Input</td>
                                                    <td>{{ input_name }}</td>
                                                    <td>{{ input_data.id }}</td>
                                                    <td>{{ input_data.uuid }}</td>
                                                </tr>
                                                {% endfor %}
                                                
                                                {% for output_name, output_data in job_data.INFO.outputs.items() %}
                                                <tr>
                                                    <td>Output</td>
                                                    <td>{{ output_name }}</td>
                                                    <td>{{ output_data.id }}</td>
                                                    <td>{{ output_data.uuid }}</td>
                                                </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        {% endfor %}
                    {% endif %}
                </div>


            </div>
            {% endfor %}
        </div>
        {% endfor %}
    </div>

    <script>
        function switchTab(tabId) {
            // Get the server and compute ID from the tab ID
            const parts = tabId.split('-');
            const tabType = parts[parts.length - 1]; // 'successful', 'failed', or 'timeout'
            const serverComputeId = tabId.substring(0, tabId.length - tabType.length - 1);
            
            // Hide all tab contents for this server/compute ID
            const tabContents = document.querySelectorAll(`[id^="${serverComputeId}-"]`);
            tabContents.forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Show the selected tab content
            document.getElementById(tabId).classList.add('active');
            
            // Update tab styles
            const tabContainer = document.getElementById(`tabs-${serverComputeId}`);
            const tabs = tabContainer.querySelectorAll('.tab');
            tabs.forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Find and activate the clicked tab
            const activeTabIndex = (tabType === 'successful') ? 0 : (tabType === 'failed' ? 1 : 2);
            tabs[activeTabIndex].classList.add('active');
        }
    </script>
</body>
</html>